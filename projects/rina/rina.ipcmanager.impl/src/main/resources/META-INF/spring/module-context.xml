<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
			http://www.springframework.org/schema/beans
			http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
			
			<bean name="idd" class="rina.ipcmanager.impl.StaticInterDIFDirectory"/>
			<bean name="ipcmanager" class="rina.ipcmanager.impl.IPCManagerImpl">
				<property name="IPCProcessFactory" ref="ipcProcessFactoryService" />
				<property name="InterDIFDirectory" ref="idd"/>
			</bean>
</beans>
