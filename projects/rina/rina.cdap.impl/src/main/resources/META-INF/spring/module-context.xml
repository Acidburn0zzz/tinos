<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="
			http://www.springframework.org/schema/beans
			http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	<bean name="wireMessageProviderFactoryBean" class="rina.cdap.impl.googleprotobuf.GoogleProtocolBufWireMessageProviderFactory"/>
	<bean name="cdapSessionManagerFactoryBean" class="rina.cdap.impl.CDAPSessionManagerFactoryImpl">
		<property name="wireMessageProviderFactory" ref="wireMessageProviderFactoryBean"/>
	</bean>
</beans>
